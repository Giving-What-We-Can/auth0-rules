{
  "name": "auth0-rules",
  "version": "1.0.0",
  "private": true,
  "description": "Update EffectiveAltruism Auth0 rules programmatically",
  "license": "MIT",
  "author": "colophonemes <sam@centreforeffectivealtruism.org>",
  "main": "index.ts",
  "scripts": {
    "build": "yarn build:cli && yarn build:rules",
    "build:cli": "yarn tsc",
    "build:cli:watch": "yarn tsc --watch",
    "build:rules": "yarn tsc -p rules/tsconfig.json",
    "build:rules:watch": "yarn build:rules --watch",
    "rules": "node -r dotenv/config dist/index"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "auth0": "^2.34.1",
    "chalk": "^4.1.0",
    "diff": "^5.0.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "@tepez/auth0-rules-types": "^0.3.1",
    "@types/auth0": "^2.33.3",
    "@types/diff": "^5.0.0",
    "@types/mz": "^2.7.3",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "commander": "^7.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-jsdoc": "^0.3.18",
    "prettier-plugin-packagejson": "^2.2.10",
    "prettier-plugin-sort-json": "^0.0.2",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "^14.11.x"
  }
}
